{
    "type": "module",
    "name": "purifyjs",
    "version": "0.0.0",
    "main": "./lib/all.js",
    "types": "./types/all.d.ts",
    "workspaces": [
        "./apps/*",
        "./*"
    ],
    "scripts": {
        "format": "prettier -w ./",
        "types": "tsc ./lib/*.js --declaration --allowJs --emitDeclarationOnly --outDir types"
    },
    "devDependencies": {
        "@eslint/js": "^9.2.0",
        "eslint": "^8.57.0",
        "prettier": "latest",
        "prettier-plugin-organize-imports": "latest",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.8.0"
    }
}
