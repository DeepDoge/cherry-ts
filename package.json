{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "export NODE_ENV=dev && node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "serve": "node server"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "chalk": "^5.1.2",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.0.11",
    "tsx": "^3.10.4",
    "typescript": "^4.6.4",
    "typescript-util-types": "github:DeepDoge/typescript-util-types",
    "vite": "^3.1.0"
  }
}
